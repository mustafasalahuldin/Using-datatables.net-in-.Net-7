{
  "compilerOptions": {
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "lib": [ "es5", "es2015", "es2016", "es2017", "es2018", "ES2018.Promise", "dom" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
